{
	"name": "my-app",
	"version": "1.0.0",
	"description": "Production-ready Express TypeScript application",
	"main": "dist/server.js",
	"type": "commonjs",
	"scripts": {
		"dev": "tsx watch src/server.ts",
		"build": "tsc && tsc-alias",
		"start": "node --enable-source-maps dist/server.js",
		"start:dev": "tsx watch src/server.ts",
		"lint": "eslint src --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"clean": "rimraf dist",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"seed": "tsx src/config/seed.ts",
    	"parse": "tsx src/config/parse.ts"
	},
	"keywords": [
		"express",
		"typescript",
		"api"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"compression": "^1.8.1",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"express-rate-limit": "^8.2.1",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^8.0.0",
		"morgan": "^1.10.1",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.2",
		"supertest": "^7.1.4",
		"ts-node": "^10.9.2",
		"zod": "^3.22.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/bcryptjs": "^2.4.0",
		"@types/compression": "^1.8.1",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.0",
		"@types/morgan": "^1.9.10",
		"@types/node": "^24.10.0",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^8.46.3",
		"@typescript-eslint/parser": "^8.46.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"jest": "^30.2.0",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.6",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3"
	}
}
